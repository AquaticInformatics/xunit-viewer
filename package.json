{
  "name": "junit-viewer",
  "version": "4.4.0",
  "description": "A simple and quick tool to view junit results",
  "main": "src/main.js",
  "bin": {
    "junit-viewer": "./bin/junit-viewer"
  },
  "directories": {
    "bin": "./bin",
    "templates": "./templates",
    "src": "./src"
  },
  "keywords": [
    "junit",
    "xunit",
    "test"
  ],
  "scripts": {
    "test": "jest && ./bin/junit-viewer --results=junit_viewer_specs --save=junit_viewer_specs.html"
  },
  "author": "Luke Preston <lukejpreston@gmail.com>",
  "license": "ISC",
  "repository": {
    "url": "https://github.com/lukejpreston/junit_viewer.git",
    "type": "git"
  },
  "bugs": {
    "url": "https://github.com/lukejpreston/junit_viewer.git"
  },
  "homepage": "https://github.com/lukejpreston/junit_viewer.git",
  "dependencies": {
    "express": "^4.12.4",
    "mustache": "^2.2.0",
    "xml2js": "^0.4.8"
  },
  "devDependencies": {
    "jasmine-reporters": "^1.0.0",
    "jest-cli": "^0.8.0"
  },
  "jest": {
    "setupTestFrameworkScriptFile": "<rootDir>/setup-jasmine-env.js",
    "collectCoverage": true,
    "coverageReporters": [
      "lcov"
    ]
  }
}
